<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="393d6d42-93f9-4155-9f5e-bae30cba1976" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>Stage Check</name>
  <description>Check for latest stage</description>
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_8" name="" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[LATEST=$(curl -s http://distfiles.gentoo.org/releases/amd64/autobuilds/latest-stage3-amd64-hardened+nomultilib.txt|tail -n1|cut -d/ -f 1)
grep ${LATEST} /var/data/catalyst/builds/stage-snapshots-seen.log && exit 1
echo "New Seed: ${LATEST}" && exit 0]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

