<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="4f366008-7bbf-4f98-8aaa-3b05f9b42045" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>Catalyst Factory</name>
  <description>Gentoo Catalyst Build Factory</description>
  <settings>
    <parameters>
      <param name="target.disable.multilib" value="-n" spec="checkbox description='Disable Multilib Support' label='Disable Multilib' display='prompt' checkedValue='-n'" />
      <param name="target.enable.debug" value="" spec="checkbox description='Enable Debugging' label='Enable Debugging' display='prompt' checkedValue='-d'" />
      <param name="target.enable.selinux" value="" spec="checkbox description='Enable Security Enhanced Linux' label='Enable Selinux' display='prompt' checkedValue='-s'" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_55" name="Genisys" type="simpleRunner">
        <parameters>
          <param name="script.content" value="sudo /usr/local/bin/geni -T stage -S all -A amd64 -K %target.kernel% -N %target.name% -P %target.profile% -q -vvv %target.disable.multilib% %target.enable.selinux% %target.enable.debug%" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Gentoo_Catalyst_GentooCatalyst" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

