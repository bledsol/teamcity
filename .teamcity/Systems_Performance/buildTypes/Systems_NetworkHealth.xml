<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="9562a2d3-abc3-4106-8350-de60a6c95a35" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>Network Health</name>
  <description>Route Health</description>
  <settings>
    <options>
      <option name="artifactRules" value="./mtr_out.txt" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_17" name="ansible the things" type="simpleRunner">
        <parameters>
          <param name="script.content" value="/usr/sbin/mtr -c 10 -r google.com &gt; mtr_out.txt" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_21" name="TCP Traceroute" type="simpleRunner">
        <parameters>
          <param name="script.content" value="/usr/sbin/tcptraceroute -q 10 google.com 80" />
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ansible">
        <checkout-rule rule="+:.=&gt;ansible" />
      </vcs-entry-ref>
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_3" type="schedulingTrigger">
        <parameters>
          <param name="cronExpression_dm" value="*" />
          <param name="cronExpression_dw" value="*" />
          <param name="cronExpression_hour" value="*" />
          <param name="cronExpression_min" value="0" />
          <param name="cronExpression_month" value="*" />
          <param name="cronExpression_sec" value="0" />
          <param name="cronExpression_year" value="*" />
          <param name="dayOfWeek" value="Sunday" />
          <param name="hour" value="12" />
          <param name="minute" value="0" />
          <param name="promoteWatchedBuild" value="true" />
          <param name="revisionRule" value="lastFinished" />
          <param name="revisionRuleBuildBranch" value="&lt;default&gt;" />
          <param name="schedulingPolicy" value="cron" />
          <param name="timezone" value="SERVER" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="perfmon" type="perfmon">
        <parameters />
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

